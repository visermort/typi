{
    "name": "typicms/base",
    "type": "project",
    "description": "Multilingual CMS built with Laravel 5",
    "keywords": ["cms", "multilingual", "laravel", "typi"],
    "license": "MIT",
    "authors": [
        {
            "name": "Samuel De Backer",
            "email": "samuel@typidesign.be",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/TypiCMS/croppa.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/TypiCMS/Laravel-Sidebar.git"
        }
    ],
    "require": {
        "php": "^7.2",
        "bkwld/croppa": "dev-master",
        "eluceo/ical": "^0.15.1",
        "fideloper/proxy": "^4.0",
        "genealabs/laravel-model-caching": "^0.7.0",
        "guzzlehttp/guzzle": "^6.3",
        "laracasts/presenter": "~0.2.2",
        "laracasts/utilities": "~3.0.1",
        "laravel/framework": "^6.0",
        "laravel/tinker": "^1.0",
        "laravelium/feed": "^6.0",
        "laravelium/sitemap": "^6.0",
        "maatwebsite/laravel-sidebar": "~1.1.4",
        "msurguy/honeypot": "~1.1.2",
        "spatie/eloquent-sortable": "~3.7.0",
        "spatie/laravel-permission": "^3.0",
        "spatie/laravel-query-builder": "^2.1",
        "spatie/laravel-translatable": "~4.2.0",
        "typicms/blocks": "~7.0",
        "typicms/core": "~7.0",
        "typicms/dashboard": "~7.0",
        "typicms/files": "~7.0",
        "typicms/history": "~7.0",
        "typicms/laravel-translatable-bootforms": "~4.1",
        "typicms/menus": "~7.0",
        "typicms/nestablecollection": "~1.1.15",
        "typicms/objects": "~7.0",
        "typicms/pages": "~7.0",
        "typicms/roles": "~7.0",
        "typicms/settings": "~7.0",
        "typicms/sitemap": "~7.0",
        "typicms/tags": "~7.0",
        "typicms/translations": "~7.0",
        "typicms/users": "~7.0",
        "typidesign/laravel-artisan-translations": "~1.0.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "TypiCMS\\Modules\\": "Modules/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php artisan storage:link"
        ]
    }
}
